/* pages/change-plans/change-plans.wxss */
.plans_title{margin:20px 0;width:90px;position:relative;height:20px;}
.plans_title image{width:100%;height:100%;}
.plans_title text{position:absolute;left:10px;line-height:20px;color:#fff;}
.plans_content{padding-left:10px;}
.plans_item{display:inline-block;padding:2px 4px;border:1px solid #535353;border-radius:5px;text-align:center;margin-right:4px;margin-bottom:4px;color:#666;}
.play_title{line-height:36px;border-bottom:1px dashed #f00;padding-left:14px;margin-bottom:10px;}
.active{background-color:#f00;color:#fff;border:none;}