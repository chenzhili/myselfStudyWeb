/* pages/agency/agency-center.wxss */
.account_title{padding:10px 0;width:100%;height:130px;}
.account_title .title{text-align:center;color:#fff;line-height:30px;}
.account_title .user_img{width:70px;height:70px;border-radius:50%;margin:0 auto;overflow:hidden;}
.account_title .user_img image{width:100%;height:100%;}
.agency_details{width:100%;background-color:#35BEF5;height:50px;}
.agency_details .agency_details_item{width:25%;height:100%;padding:4px 0px;text-align:center;color:#fff;}
.agency_details .agency_details_item>view{margin:0;padding:0;}
.agency_details .agency_details_item>view:last-child{font-size:12px;}
.account_content{background-color:#fff;}
.account_item{width:33.3%;height:100px;}
.account_img{width:46%;margin:20px auto 10px auto;text-align:center;font-size:12px;color:#000;}
.account_img image{width:70%;vertical-align:middle;}
.account_img view{margin-top:6px;}